#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_@cloudflare_wrangler@1.19.11@@cloudflare/wrangler/run-wrangler.js" "$@"
  ret=$?
else 
  node  "$basedir/../_@cloudflare_wrangler@1.19.11@@cloudflare/wrangler/run-wrangler.js" "$@"
  ret=$?
fi
exit $ret
